{
  "name": "vue-starter",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite",
    "start": "vite --port 3333",
    "build": "vue-tsc --build --force && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "postinstall": "npx simple-git-hooks"
  },
  "dependencies": {
    "@unocss/reset": "^0.58.3",
    "ant-design-vue": "^4.0.8",
    "pinia": "^2.1.7",
    "vue": "^3.4.3",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.1",
    "@types/node": "^18.19.4",
    "@unocss/eslint-config": "^0.58.3",
    "@vitejs/plugin-vue": "^5.0.2",
    "eslint": "^8.56.0",
    "eslint-plugin-format": "^0.1.0",
    "lint-staged": "^15.2.0",
    "simple-git-hooks": "^2.9.0",
    "typescript": "~5.3.3",
    "unocss": "^0.58.3",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.10",
    "vitest": "^1.1.1",
    "vue-tsc": "^1.8.27"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
